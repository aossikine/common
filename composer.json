{
	"name": "fuelphp/common",
	"description": "FuelPHP generic helper functions and classes.",
	"keywords": ["Helper", "Utility"],
	"homepage": "https://github.com/fuelphp/common",
	"license": "MIT",
	"authors": [
		{
			"name": "FuelPHP Development Team",
			"email": "team@fuelphp.com"
		}
	],
	"require": {
		"php": ">=5.4"
	},
	"require-dev": {
		"codeception/codeception": "~2.0",
		"symfony/yaml": "^2.3"
	},
	"suggest": {
		"symfony/yaml": "Allows YAML data conversions."
	},
	"autoload": {
		"psr-4": {
			"Fuel\\Common\\": "src/"
		},
		"files": ["helpers/bootstrap.php"]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	}
}
